# Environment Variables for Monad Code Arena

# Monad Testnet Configuration (September 2025)
MONAD_TESTNET_RPC_URL=https://testnet-rpc.monad.xyz
PRIVATE_KEY=your_deployer_private_key_here
DEPLOYER_ADDRESS=your_deployer_address_here

# Oracle Configuration 
ORACLE_PRIVATE_KEY=your_oracle_private_key_here
ORACLE_ADDRESS=your_oracle_address_here

# Contract Addresses (will be filled after deployment)
GAME_TOKEN_ADDRESS=
ARENA_CONTRACT_ADDRESS=
BADGES_CONTRACT_ADDRESS=

# Database
DATABASE_URL=file:./monad_arena.db

# API Configuration
BACKEND_URL=http://localhost:3001
FRONTEND_URL=http://localhost:3000

# Security
JWT_SECRET=your_jwt_secret_here_min_32_characters
ENCRYPTION_KEY=your_32_character_encryption_key_

# Match Configuration
MATCH_TIMEOUT_SECONDS=300
PROTOCOL_FEE_BPS=250
MIN_STAKE_AMOUNT=1000000000000000000

# Development
NODE_ENV=development
LOG_LEVEL=debug

